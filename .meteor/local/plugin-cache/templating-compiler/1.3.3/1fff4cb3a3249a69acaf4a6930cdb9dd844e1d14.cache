{"head":"<title>Shopping List</title>\n    <link href=\"https://fonts.googleapis.com/icon?family=Material+Icons\" rel=\"stylesheet\">\n    <meta charset=\"utf-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">","body":"","js":"\nTemplate.body.addContent((function() {\n  var view = this;\n  return [ HTML.NAV({\n    class: \"teal\"\n  }, \"\\n        \", HTML.DIV({\n    class: \"container\"\n  }, \"\\n            \", HTML.DIV({\n    clas: \"nav-wrapper\"\n  }, \"\\n                \", HTML.Raw('<a href=\"#\" class=\"brand-logo\">iBuy</a>'), \"\\n                \", HTML.UL({\n    id: \"nav-mobile\",\n    class: \"right\"\n  }, \"\\n                    \", Blaze.If(function() {\n    return Spacebars.call(view.lookup(\"currentUser\"));\n  }, function() {\n    return [ \"\\n                        \", HTML.LI({\n      class: \"nav-item\"\n    }, \"\\n                            \", HTML.A({\n      class: \"waves-effect waves-light btn modal-trigger\",\n      href: \"#addModal\"\n    }, \" Add Item\"), \"\\n                        \"), \"\\n                    \" ];\n  }), \"\\n                    \", HTML.LI({\n    class: \"nav-item\"\n  }, \"\\n                        \", Spacebars.include(view.lookupTemplate(\"loginButtons\")), \"\\n                    \"), \"\\n                \"), \"\\n            \"), \"\\n        \"), \"\\n    \"), \"\\n    \", HTML.DIV({\n    class: \"container\"\n  }, \"\\n        \", HTML.Raw(\"<h1>Items</h1>\"), \"\\n        \", Blaze.If(function() {\n    return Spacebars.call(view.lookup(\"currentUser\"));\n  }, function() {\n    return [ \"\\n            \", Spacebars.include(view.lookupTemplate(\"sort\")), \"\\n            \", HTML.UL({\n      class: \"collection\"\n    }, \"\\n                \", HTML.LI({\n      class: \"collection-item\"\n    }, HTML.STRONG({\n      style: \"margin-right: 40%\"\n    }, \"a\"), HTML.STRONG({\n      style: \"color: black; margin-right: 10%\"\n    }, \"a\"), \"\\n                \"), \"\\n                \", Blaze.Each(function() {\n      return Spacebars.call(view.lookup(\"items\"));\n    }, function() {\n      return [ \"\\n                    \", Spacebars.include(view.lookupTemplate(\"item\")), \"\\n                \" ];\n    }), \"\\n            \"), \"\\n        \" ];\n  }, function() {\n    return [ \"\\n            \", HTML.P(\"Please log in to manage your shopping list.\"), \"\\n        \" ];\n  }), \"\\n    \"), \"\\n\\n    \", Spacebars.include(view.lookupTemplate(\"add\")), \"\\n\\n    \", HTML.SCRIPT(\"\\n        $(document).ready(function(){\\n            $('.modal').modal();\\n        });\\n    \") ];\n}));\nMeteor.startup(Template.body.renderToDocument);\n\nTemplate.__checkName(\"item\");\nTemplate[\"item\"] = new Template(\"Template.item\", (function() {\n  var view = this;\n  return HTML.LI({\n    class: \"collection-item \"\n  }, \"\\n        \", Blaze.View(\"lookup:name\", function() {\n    return Spacebars.mustache(view.lookup(\"name\"));\n  }), \"\\n        \", Blaze.View(\"lookup:quantity\", function() {\n    return Spacebars.mustache(view.lookup(\"quantity\"));\n  }), \"\\n        \", Blaze.View(\"lookup:priority\", function() {\n    return Spacebars.mustache(view.lookup(\"priority\"));\n  }), HTML.Raw('\\n        <a href=\"#\" class=\"delete-item secondary-content\"><i class=\"material-icons\">done</i></a>\\n    '));\n}));\n\nTemplate.__checkName(\"add\");\nTemplate[\"add\"] = new Template(\"Template.add\", (function() {\n  var view = this;\n  return HTML.Raw('<!-- Modal Structure -->\\n    <div id=\"addModal\" class=\"modal\">\\n        <div class=\"modal-content\">\\n            <h3>Add Item</h3>\\n            <form class=\"add-form\">\\n                <input type=\"text\" name=\"text\" placeholder=\"Item Name\" style=\"width: 60%\">\\n                <input type=\"text\" name=\"quantity\" placeholder=\"QTY\" style=\"width: 10%\">\\n                <input type=\"text\" name=\"priority\" placeholder=\"Priority\" style=\"width: 20%\">\\n                <!-- TODO: Figure out a dropdown for quantity. -->\\n                <!-- <select name=\"quantity\" size=\"3\" style=\"width: 20%\">\\n                    <option value=\"1\" selected>1</option>\\n                    <option value=\"2\">2</option>\\n                    <option value=\"3\">3</option>\\n                    <option value=\"4\">4</option>\\n                </select> -->\\n                <input type=\"submit\" value=\"Add\" class=\"add modal-close waves-effect waves-green btn-flat\">\\n            </form>\\n        </div>\\n    </div>');\n}));\n\nTemplate.__checkName(\"sort\");\nTemplate[\"sort\"] = new Template(\"Template.sort\", (function() {\n  var view = this;\n  return HTML.Raw('Sort By:\\n    <button type=\"button\" class=\"priority-sort\">Priority</button>\\n    <button type=\"button\" class=\"name-sort\">Name</button>');\n}));\n","bodyAttrs":{}}